# MISP repositories
[misp]
name=misp
baseurl=https://repo.misp-project.ch/yum/misp
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-KOJI-SWITCH
	file:///etc/pki/rpm-gpg/RPM-GPG-KEY-KOJI-MISPPROJECT
gpgcheck=1

[mariadb]
name=mariadb
baseurl=http://yum.mariadb.org/11.4/centos7-amd64/
enabled=1
gpgkey=https://yum.mariadb.org/RPM-GPG-KEY-MariaDB
gpgcheck=1
