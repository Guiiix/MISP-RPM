# MISP repositories
[mariadb]
name=mariadb
baseurl=https://yum.mariadb.org/11.4/rhel8-amd64
enabled=1
module_hotfixes=1
gpgkey=https://yum.mariadb.org/RPM-GPG-KEY-MariaDB
gpgcheck=1

[misp]
name=misp
baseurl=https://repo.misp-project.ch/yum/misp8
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-KOJI-SWITCH
	file:///etc/pki/rpm-gpg/RPM-GPG-KEY-KOJI-MISPPROJECT
gpgcheck=1
